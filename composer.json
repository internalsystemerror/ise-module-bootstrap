{
    "name": "ise/ise-module-bootstrap",
    "description": "Twitter Bootstrap module for Zend Framework 2 used by Internalsystemerror Limited",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/internalsystemerror/ise-module-bootstrap",
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-eventmanager": "^2.5",
        "zendframework/zend-form": "^2.5",
        "zendframework/zend-json": "^2.5",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-mvc": "^2.5",
        "zendframework/zend-navigation": "^2.5",
        "zendframework/zend-view": "^2.5",
        "rwoverdijk/assetmanager": "^1.7",
        "nervo/yuicompressor": "2.4.7"
    },
    "require-dev": {
        "zendframework/zftool": "^0.1 || ^1.0",
        "zendframework/zenddiagnostics": "^1.0",
        "ise/php-metapackage-development": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IseBootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IseBootstrapTest\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs",
            "@phpmd",
            "@phpcpd"
        ],
        "phpunit": "phpunit -c ./phpunit.xml.dist",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "phpmd": "phpmd . html --reportfile ./build/logs/phpmd.html --suffixes php,phtml --exclude vendor,build ./phpmd.xml.dist",
        "phpcpd": "phpcpd . --exclude=vendor --exclude=data"
    }
}
